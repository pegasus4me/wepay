{
  "name": "weppo",
  "version": "1.0.0",
  "description": "The Consumer Abstraction Layer for Autonomous AI Agent Payments",
  "private": true,
  "workspaces": [
    "packages/sdk",
    "api",
    "interface",
    "demo"
  ],
  "scripts": {
