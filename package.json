    "demo": "npx tsx demo/agent-pay.ts",
    "demo:commerce": "npx tsx demo/agent-commerce/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pegasus4me/wepay.git"
  },
