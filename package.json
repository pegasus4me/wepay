  "bugs": {
    "url": "https://github.com/pegasus4me/wepay/issues"
  },
  "homepage": "https://github.com/pegasus4me/wepay#readme",
  "dependencies": {
    "awal": "^2.0.3",
    "better-sqlite3": "^12.6.2",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "viem": "^2.46.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
